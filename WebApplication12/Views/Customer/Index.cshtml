@model IEnumerable<WebApplication12.Models.Location>
@{
}

<table class="table table-bordered">
    <tr>
        <th>Locations</th>
        <th>Details</th>
    </tr>
    @foreach(var location in Model)
            {
    <tr>
    <td><a asp-action="CustomerList" asp-controller="Customer" asp-route-id="@location">  @location.Place</a></td>
    <td>@location.Detail</td>
        </tr>    
        }

</table>